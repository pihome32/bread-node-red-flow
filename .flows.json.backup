    {
        "id": "502ac72e.0a62c",
        "type": "function",
        "z": "fcbfcd1c.3389b8",
        "name": "",
        "func": "\nmsg.enabled = true;\nvar time = msg.timerend;\nvar dt = new Date(time - msg.payload);\nvar minutes = dt.getMinutes();\nvar seconds = dt.getSeconds();\nvar hours = dt.getHours() - 1;\nif (minutes < 10) {\n        minutes = \"0\" + minutes;\n}\nif (seconds < 10) {\n        seconds = \"0\" + seconds;\n}\nif (hours < 10) {\n        hours = \"0\" + hours;\n} else if (hours== 1) {\n        hours = \"00\";\n}\nmsg.payload = hours+\":\"+minutes + \":\" + seconds;\nmsg.label=\"<i class='fa fa-clock-o fa-2x'></i>  \";\n\nif ((time-msg.payload)<100){\n    global.set(\"state.setpoint.value\",msg.setpoint);\n}    \nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 1040,
